generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model rallies {
  id        Int       @id @default(autoincrement())
  name      String?   @db.Text
  season    Int?
  country   String?   @db.Text
  beginning DateTime? @db.Date
  end       DateTime? @db.Date
}

model wrc_drivers {
  id        Int     @id @default(autoincrement())
  firstName String? @db.Text
  lastName  String? @db.Text
  country   String? @db.Text
  team      String? @db.Text
}
