generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model rallies {
  id        Int       @id @default(autoincrement())
  name      String?   @db.Text
  season    Int?
  country   String?   @db.Text
  beginning DateTime? @db.Date
  end       DateTime? @db.Date
}
